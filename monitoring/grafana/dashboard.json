    {
      "type": "graph",
      "title": "API Request Count",
      "targets": [
        { "expr": "sum by (endpoint) (increase(api_request_count[5m]))", "legendFormat": "{{endpoint}}" }
      ],
      "gridPos": { "x": 6, "y": 0, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "API Latency (p95)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(api_request_latency_seconds_bucket[5m])) by (le, endpoint))", "legendFormat": "{{endpoint}}" }
      ],
